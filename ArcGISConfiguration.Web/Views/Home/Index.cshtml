@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>ArcGIS Config Sample</title>
    <meta name="viewport" content="initial-scale=1, maximum-scale=1,user-scalable=no">
    <link rel="stylesheet" href="//serverapi.arcgisonline.com/jsapi/arcgis/3.3/js/dojo/dijit/themes/claro/claro.css" />
    <link rel="stylesheet" href="//serverapi.arcgisonline.com/jsapi/arcgis/3.3/js/esri/css/esri.css" />
    <link rel="stylesheet" href="~/Content/App.css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body class="claro">
    <div id="mainWindow"
        data-dojo-type="dijit.layout.BorderContainer"
        data-dojo-props="design:'headline', gutters:false"
        style="width: 100%; height: 100%;">
        <div id="header"
            data-dojo-type="dijit.layout.ContentPane"
            data-dojo-props="region:'top'">
            <div style="float: left">
                @Html.ActionLink("test 1", "Index", new { role = "basemap" })
                @Html.ActionLink("test 2", "Index", new { role = "operational" })
            </div>
            <h2>ArcGIS Config Sample</h2>
        </div>
        <div id="map"
            data-dojo-type="dijit.layout.ContentPane"
            data-dojo-props="region:'center'">
        </div>
    </div>
    <script defer src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        var dojoConfig = { async: true };
        var role = "@TempData["Role"]";
    </script>
    <script defer src="//serverapi.arcgisonline.com/jsapi/arcgis/3.3compact/"></script>
    <script defer src="~/Scripts/mapManager.js"></script>
    <script defer src="~/Scripts/app.js"></script>
</body>
</html>
